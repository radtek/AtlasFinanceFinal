<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="StockServer.HTTP" />
        <binding name="NuCardAdmin.HTTP" />
      </basicHttpBinding>
      <netTcpBinding>
        <binding name="StockServer.NET">
          <security mode="None" />
        </binding>
        <binding name="NuCardAdmin.NET">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://localhost:8200/StockServer" binding="netTcpBinding" bindingConfiguration="StockServer.NET" contract="StockServer.IStockServer" name="StockServer.NET" />
      <endpoint address="http://localhost:8201/StockServer" binding="basicHttpBinding" bindingConfiguration="StockServer.HTTP" contract="StockServer.IStockServer" name="StockServer.HTTP" />
