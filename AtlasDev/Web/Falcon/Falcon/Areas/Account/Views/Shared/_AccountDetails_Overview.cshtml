  <section class="panel-body">
    <div class="horizontal-form">
      <fieldset>
        <legend>Overview</legend>
        <div class="row">
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_account_no">Account No</label>
              <input class="form-control" id="control_account_no" name="control[account_no]" type="text" value="{{ account.AccountNo }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_host">Host</label>
              <input class="form-control" id="control_host" name="control[Host]" type="text" value="{{ account.Host }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
          </div>
          <div class="col-sm-2">
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_account_type">Account Type</label>
              <input class="form-control" id="control_account_type" name="control[account_type]" type="text" value="{{ account.AccountType }}" ng-readonly="true">
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_status">Status</label>
              <input class="form-control" id="control_status" name="control[status]" type="text" value="{{ account.Status }}" ng-style="{'background-color': accountStatus(account.Status)}" ng-readonly="true">
            </div>
          </div>
        </div>
      </fieldset>
      <fieldset>
        <legend>Summary</legend>
        <div class="row">
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Interest Rate p.a.</label>
              <input class="form-control" id="control_InterestRate" name="control[InterestRate]" type="text" value="{{ account.InterestRate }} %" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Applied Amount</label>
              <input class="form-control" id="control_LoanAmount" name="control[LoanAmount]" type="text" value="{{ account.LoanAmount | currency }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Capital Amount</label>
              <input class="form-control" id="control_CapitalAmount" name="control[CapitalAmount]" type="text" value="{{ account.CapitalAmount | currency }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Payout Amount</label>
              <input class="form-control" id="control_PayoutAmount" name="control[PayoutAmount]" type="text" value="{{ account.PayoutAmount | currency }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Current Balance</label>
              <input class="form-control" id="control_AccountBalance" name="control[AccountBalance]" type="text" value="{{ account.AccountBalance | currency }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Instalment Amount</label>
              <input class="form-control" id="control_InstalmentAmount" name="control[InstalmentAmount]" type="text" value="{{ account.InstalmentAmount | currency }}" ng-readonly="true" />
            </div>
          </div>  
        </div>
      </fieldset>
      <fieldset>
        <legend>Financial</legend>
        <div class="row">
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Period</label>
              <input class="form-control" id="control_period" name="control[period]" type="text" value="{{ account.Period }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Frequency</label>
              <input class="form-control" id="control_frequency" name="control[frequency]" type="text" value="{{ account.PeriodFrequency }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_day_of_month">Pay Rule</label>
              <input class="form-control" id="control_payrule" name="control[payrule]" type="text" value="{{ account.PayRule }}" ng-readonly="true"/>
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_day_of_month">Pay Date</label>  
              <input class="form-control" id="control_paydate" name="control[paydate]" type="text" value="{{ account.PayDate }}" ng-readonly="true"/>
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_first_instalment_date">First Instalment Date</label>
              <input class="form-control" id="control_first_instalment_date" name="control[first_instalment_date]" type="text" value="{{ account.FirstInstalmentDate  | date:'yyyy-MM-dd' }}" ng-readonly="true">
            </div>
          </div>
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_last_instalment_date">Last Instalment Date</label>
              <input class="form-control" id="control_last_instalment_date" name="control[last_instalment_date]" type="text" value="{{ lastInstalmentDate  | date:'yyyy-MM-dd' }}" ng-readonly="true">
            </div>
          </div>
        </div>
      </fieldset>
      <fieldset>
        <legend>Arrears Info</legend>
        <div class="row">
          <div class="col-sm-2">
            <div class="form-group">
              <label for="control_frequency">Arrears Amount</label>
              <input class="form-control" id="control_ArrearsAmount" name="control[ArrearsAmount]" type="text" value="{{ account.ArrearsAmount | currency }}" ng-readonly="true" />
            </div>
          </div>
          <div class="col-sm-10 form-group" >
            <div class="slider-value" style="padding-bottom:13px">
              {{getDelinquency()}} Delinquent
            </div>
            @*<div id="slider-snap-inc" class="slider bg-red ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all" aria-disabled="false">
              <div class="ui-slider-range ui-widget-header ui-corner-all ui-slider-range-max" ng-style="myprop(true)"></div>
              <a class="ui-slider-handle ui-state-default ui-corner-all" href="#" ng-style="myprop(false)"></a>
            </div>*@
            <div class="progress">
              <div class="progress-bar progress-bar-danger" ng-style="progressDeliquency(true)">
                <span class="sr-only">
                  35% Complete (success)
                </span>
              </div>
              <div class="progress-bar progress-bar-info" ng-style="progressDeliquency(false)">
                <span class="sr-only">
                  20% Complete (warning)
                </span>
              </div>
            </div>
            @*<div ng-style="myprop()" class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">
              <span class="sr-only">
                {{account.Deliquency}} month(s)
              </span>
            </div>*@
          </div>
        </div>
      </fieldset>
    </div>
  </section>
